    "gatsby-plugin-resolve-src": "^2.0.0-beta.1",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-styled-components": "^3.1.19",
    "gatsby-plugin-typography": "^2.3.22",
